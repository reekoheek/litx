{
  "name": "testutil",
  "private": true,
  "version": "2.5.3",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "wireit",
    "lint:eslint": "wireit",
    "lint:ts": "wireit",
    "build": "wireit",
    "test": "vitest run --coverage"
  },
  "wireit": {
    "lint": {
      "dependencies": [
        "lint:eslint",
        "lint:ts"
      ]
    },
    "lint:eslint": {
      "command": "eslint ./src",
      "files": [
        "./src/**/*.ts"
      ],
      "output": []
    },
    "lint:ts": {
      "command": "tsc --noEmit",
      "files": [
        "./src/**/*.ts"
      ],
      "output": []
    },
    "build": {
      "command": "tsc",
      "dependencies": [
        "lint:eslint"
      ],
      "files": [
        "./src/**/*.ts"
      ],
      "output": []
    }
  },
  "keywords": [],
  "author": "Ganesha <reekoheek@gmail.com>",
  "license": "MIT"
}
